        <Label Content="Stawka podatkowa" HorizontalAlignment="Left" Margin="9,60,0,0" VerticalAlignment="Top"  Height="26" Width="121" />
        <TextBox Name="TaxRateTextBox" HorizontalAlignment="Left" Height="23" Margin="147,60,0,0"
                 VerticalAlignment="Top" Width="124"  KeyDown="OnKeyDownHandler" />
        <Button Content="Dodaj" Name="AddButton" HorizontalAlignment="Left" Margin="43,101,0,0" VerticalAlignment="Top"
                Width="75" Click="AddButton_Click"  Height="20" />
        <Button Content="Anuluj" Name="CloseButton" HorizontalAlignment="Left" Margin="172,101,0,0"
                VerticalAlignment="Top" Width="75" Click="CloseButton_Click"  Height="20" />
    </Grid>
</Window>